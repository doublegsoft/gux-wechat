<gx-navigation-bar id="navigationBar" title="首页" backable="{{false}}"></gx-navigation-bar>
<view style="margin-top: {{marginTop}}px;">
  <view class="gx-m-8 gx-fs-14">
    <text class="fas fa-list-ul"></text>
    <text class="gx-ml-8">列表示例</text>
  </view>
  <view>
    <navigator bindtap="gotoListView_NoData" url="#" class="gx-tile gx-py-12 gx-fs-14" role="navigator">
      <view class="gx-tile-body" style="width: 200px;">传统列表（无数据）</view>
      <view class="fas fa-angle-right gx-fs-18 access"></view>
    </navigator>
    <navigator bindtap="gotoListView_LoadMore" url="#" class="gx-tile gx-py-12 gx-fs-14" role="navigator">
      <text class="gx-tile-body">传统列表（下拉刷新）</text>
      <view class="fas fa-angle-right gx-fs-18 access"></view>
    </navigator>
    <navigator bindtap="gotoListView_LoadMore" url="#" class="gx-tile gx-py-12 gx-fs-14" role="navigator">
      <text class="gx-tile-body">传统列表（加载更多）</text>
      <view class="fas fa-angle-right gx-fs-18 access"></view>
    </navigator>
  </view>
  <view class="gx-m-8 gx-fs-14">
    <text class="fas fa-list-ul"></text>
    <text class="gx-ml-8">页签示例</text>
  </view>
  <view>
    <navigator bindtap="gotoTabs_Date" url="#" class="gx-tile gx-py-12 gx-fs-14" role="navigator">
      <view class="gx-tile-body" style="width: 200px;">日期页签</view>
      <view class="fas fa-angle-right gx-fs-18 access"></view>
    </navigator>
    <navigator bindtap="gotoTabs_Image" url="#" class="gx-tile gx-py-12 gx-fs-14" role="navigator">
      <view class="gx-tile-body" style="width: 200px;">图片宣传</view>
      <view class="fas fa-angle-right gx-fs-18 access"></view>
    </navigator>
    <navigator bindtap="gotoTabs_Segment" url="#" class="gx-tile gx-py-12 gx-fs-14" role="navigator">
      <view class="gx-tile-body" style="width: 200px;">分隔浏览</view>
      <view class="fas fa-angle-right gx-fs-18 access"></view>
    </navigator>
  </view>
  <view class="gx-m-8 gx-fs-14">
    <text class="fas fa-bars"></text>
    <text class="gx-ml-8">表单示例</text>
  </view>
  <view>
    <navigator bindtap="gotoForm_TwoColumnForm" url="#" class="gx-tile gx-py-12 gx-fs-14" role="navigator">
      <view class="gx-tile-body">编辑表单</view>
      <view class="fas fa-angle-right gx-fs-18 access"></view>
    </navigator>
    <navigator bindtap="gotoListView_LoadMore" url="#" class="gx-tile gx-py-12 gx-fs-14" role="navigator">
      <text class="gx-tile-body">只读表单</text>
      <view class="fas fa-angle-right gx-fs-18 access"></view>
    </navigator>
    <navigator bindtap="gotoListView_LoadMore" url="#" class="gx-tile gx-py-12 gx-fs-14" role="navigator">
      <text class="gx-tile-body">花式表单</text>
      <view class="fas fa-angle-right gx-fs-18 access"></view>
    </navigator>
  </view>
  <view class="gx-m-8 gx-fs-14">
    <text class="fas fa-file"></text>
    <text class="gx-ml-8">通用页面</text>
  </view>
  <view>
    <navigator bindtap="gotoSearch" url="#" class="gx-tile gx-py-12 gx-fs-14" role="navigator">
      <text class="gx-tile-body">搜索</text>
      <view class="fas fa-angle-right gx-fs-18 access"></view>
    </navigator>
    <navigator bindtap="gotoSuccess" url="#" class="gx-tile gx-py-12 gx-fs-14" role="navigator">
      <text class="gx-tile-body">成功提示</text>
      <view class="fas fa-angle-right gx-fs-18 access"></view>
    </navigator>
    <navigator bindtap="gotoFailure" url="#" class="gx-tile gx-py-12 gx-fs-14" role="navigator">
      <text class="gx-tile-body">错误提示</text>
      <view class="fas fa-angle-right gx-fs-18 access"></view>
    </navigator>
    <navigator bindtap="gotoNoInternet" url="#" class="gx-tile gx-py-12 gx-fs-14" role="navigator">
      <text class="gx-tile-body">没有连接</text>
      <view class="fas fa-angle-right gx-fs-18 access"></view>
    </navigator>
    <navigator bindtap="gotoThankYou" url="#" class="gx-tile gx-py-12 gx-fs-14" role="navigator">
      <text class="gx-tile-body">感谢</text>
      <view class="fas fa-angle-right gx-fs-18 access"></view>
    </navigator>
    <navigator bindtap="gotoCongratulation" url="#" class="gx-tile gx-py-12 gx-fs-14" role="navigator">
      <text class="gx-tile-body">恭喜</text>
      <view class="fas fa-angle-right gx-fs-18 access"></view>
    </navigator>
    <navigator bindtap="gotoPrivacyPolicy" url="#" class="gx-tile gx-py-12 gx-fs-14" role="navigator">
      <text class="gx-tile-body">隐私政策</text>
      <view class="fas fa-angle-right gx-fs-18 access"></view>
    </navigator>
    <navigator bindtap="gotoCertification" url="#" class="gx-tile gx-py-12 gx-fs-14" role="navigator">
      <text class="gx-tile-body">实名认证协议</text>
      <view class="fas fa-angle-right gx-fs-18 access"></view>
    </navigator>
  </view>
  <view class="gx-m-8 gx-fs-14">
    <text class="fas fa-file-image"></text>
    <text class="gx-ml-8">滑动弹出</text>
  </view>
  <view>
    <navigator bindtap="doShowBottomSheet" url="#" class="gx-tile gx-py-12 gx-fs-14" role="navigator">
      <text class="gx-tile-body">底部弹出</text>
      <view class="fas fa-angle-right gx-fs-18 access"></view>
    </navigator>
  </view>
  <view class="gx-m-8 gx-fs-14">
    <text class="fas fa-file-image"></text>
    <text class="gx-ml-8">经典页面</text>
  </view>
  <view>
    <navigator bindtap="gotoFoodOrdering" url="#" class="gx-tile gx-py-12 gx-fs-14" role="navigator">
      <text class="gx-tile-body">膳食订购</text>
      <view class="fas fa-angle-right gx-fs-18 access"></view>
    </navigator>
    <navigator bindtap="gotoEvaluationResult" url="#" class="gx-tile gx-py-12 gx-fs-14" role="navigator">
      <text class="gx-tile-body">身体评估</text>
      <view class="fas fa-angle-right gx-fs-18 access"></view>
    </navigator>
    <navigator bindtap="gotoMyOrder" url="#" class="gx-tile gx-py-12 gx-fs-14" role="navigator">
      <text class="gx-tile-body">我的订单</text>
      <view class="fas fa-angle-right gx-fs-18 access"></view>
    </navigator>
    <navigator bindtap="gotoMerchandises" url="#" class="gx-tile gx-py-12 gx-fs-14" role="navigator">
      <text class="gx-tile-body">商品橱窗</text>
      <view class="fas fa-angle-right gx-fs-18 access"></view>
    </navigator>
  </view>
</view>
<gx-bottom-sheet id="bottomSheet" height="{{600}}">
  <scroll-view scroll-y="{{true}}">
  <view class="gx-m-16 gx-fs-14">
    <view class="gx-px-12 gx-h-32 gx-b-round gx-lh-32 gx-bg-success-lighter gx-color-success" catch:tap="doHideBottomSheet">营养得很</view>
    <view catch:tap="doHideBottomSheet" class="gx-mt-8 gx-px-12 gx-h-32 gx-lh-32 gx-b-round gx-bg-success-lighter gx-color-success">非常巴适</view>
    <view catch:tap="doHideBottomSheet" class="gx-mt-8 gx-px-12 gx-h-32 gx-lh-32 gx-b-round gx-bg-success-lighter gx-color-success">非常巴适</view>
    <view catch:tap="doHideBottomSheet" class="gx-mt-8 gx-px-12 gx-h-32 gx-lh-32 gx-b-round gx-bg-success-lighter gx-color-success">非常巴适</view>
  </view>
</scroll-view>
</gx-bottom-sheet>
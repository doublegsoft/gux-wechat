page {
  /* main colors */
  /* --color-primary: #1976d2; */
  --color-primary: #3f86ff;
  --color-secondary: #9c27b0;
  --color-success: #2e7d32;
  --color-error: #d32f2f;
  --color-warning: #ed6c02;
  --color-info: #0288d1;
  --color-light: rgba(0, 0, 0, 0.2);

  --color-text-primary: rgba(0, 0, 0, 0.87);
  --color-text-primary-reverse: rgba(250, 250, 250, 1);
  --color-text-secondary: rgba(0, 0, 0, 0.6);
  --color-text-disabled: rgba(0, 0, 0, 0.38);
  --color-background: white;
  --color-divider: rgba(0, 0, 0, 0.12);

  --color-primary-light: #42a5f5;
  --color-secondary-light: #ba68c8;
  --color-success-light: #4caf50; 
  --color-error-light: #ef5350;
  
  --color-warning-light: #ff9800;
  --color-info-light: #03a9f4;

  --color-primary-dark: #1565c0;
  --color-secondary-dark: #7b1fa2;
  --color-success-dark: #1b5e20;
  --color-error-dark: #c62828;
  --color-warning-dark: #e65100;
  --color-info-dark: #01579b;

  --color-text-primary-dark: #fff;
  --color-text-secondary-dark: rgba(255, 255, 255, 0.7);
  --color-text-disabled-dark: rgba(255, 255, 255, 0.5);
  --color-background-dark: #121212;
  --color-divider-dark: rgba(255, 255, 255, 0.12);

  --color-success-lighter: rgba(46, 125, 50, 0.1);
  --color-error-lighter: rgba(211, 47, 47, 0.1);
}

.page.dark {
  color: var(--color-text-primary-dark);
  background-color: var(--color-background-dark);
}

.text-primary {
  color: var(--color-primary);
}

.text-light {
  color: var(--color-light);
}

.text-warning-light {
  color: var(--color-warning-light);
}

.btn-primary {
  background-color: transparent !important;
  border: 2px solid var(--color-primary) !important;
  color: var(--color-primary) !important;
}

.btn-primary:active {
  background-color: var(--color-primary) !important;
  color: var(--color-background) !important;
}

.btn-cancel {
  background-color: transparent !important;
  border: 2px solid var(--color-error) !important;
  color: var(--color-error) !important;
}

.btn-cancel:active {
  background-color: var(--color-error) !important;
  color: var(--color-background) !important;
}

.radio .wx-radio-input.wx-radio-input-checked::before {
  background-color: var(--color-primary) !important;
  border-color: var(--color-primary) !important;
  width: 32rpx;
  height: 32rpx;
  content: "";
  border-radius: 50%;
}

.radio .wx-radio-input.wx-radio-input-checked {
  border: 1px solid var(--color-primary) !important;
  left: 0%;
}

.radio .wx-radio-input {
  background-color: transparent !important;
  width: 32rpx;
  height: 32rpx;
  border-radius: 50%;
  content: "";
  top: 50%;
  left: 0%;
}


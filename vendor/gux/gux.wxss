  align-items: center;
}

.gx-tile:after {
  content: " ";
  position: absolute;
  left: 16px;
  right: 16px;
  top: 0;
  height: 1px;
  background-color: var(--color-divider);
  -webkit-transform-origin: 0 0;
  transform-origin: 0 0;
  -webkit-transform: scaleY(0.5);
  transform: scaleY(0.5);
  z-index: 2;
}

.gx-tile:first-child:after {
  display: none;
}


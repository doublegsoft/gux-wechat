view,
text,
button {
  box-sizing: border-box;
}
.max-btn,
.line-btn,
.primary-btn,
.grey-btn {
  width: auto !important;
  font-weight: 400;
  padding: 0 22rpx;
  font-size: 24rpx;
  border-radius: 28rpx;
  box-sizing: border-box;
  margin: 0 !important;
}
.max-btn {
  box-shadow: 0px 8rpx 12rpx 0px rgba(63, 134, 255, 0.3);
  background: #3f86ff;
  color: #fff;
  height: 56rpx;
  line-height: 56rpx;
}
.line-btn {
  height: 46rpx;
  border: 2rpx solid #3f86ff;
  background: #fff;
  color: #3f86ff;
  font-size: 22rpx;
  line-height: 44rpx;
}
.primary-btn {
  background: #3f86ff;
  color: #fff;
  height: 44rpx;
  font-size: 22rpx;
  line-height: 44rpx;
}
.grey-btn {
  background: #fff;
  color: #585858;
  height: 44rpx;
  font-size: 22rpx;
  line-height: 44rpx;
  border: 2rpx solid #eeeeee;
}
.more-img {
  width: 24rpx;
  height: 24rpx;
  margin-left: 10rpx;
}
.dli-more {
  display: flex;
  align-items: center;
  font-size: 24rpx;
  color: #989898;
  font-weight: 400;
}
.right-bottom {
  position: fixed;
  right: 0;
  bottom: 0;
  z-index: 9;
}
.icon-add {
  width: 122rpx;
  height: 122rpx;
}
.noMore {
  line-height: 50rpx;
  text-align: center;
  font-size: 24rpx;
  color: #999;
}
/* switch */
.wx-switch-input {
  width: 82rpx !important;
  height: 40rpx !important;
}
/*关闭状态*/
.wx-switch-input::before {
  width: 80rpx !important;
  height: 36rpx !important;
}
/*开启状态*/
.wx-switch-input::after {
  width: 38rpx !important;
  height: 36rpx !important;
}
.wx-checkbox-input,
.wx-radio-input {
  width: 34rpx !important;
  height: 34rpx !important;
}
.wx-radio-input.wx-radio-input-checked {
  background: #3f86ff !important;
  border-color: #3f86ff !important;
}

